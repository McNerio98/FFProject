	<parameter name="idProducto" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
a.idProducto, b.NombreProducto, SUM(a.NumUnidades) AS cantidad
FROM DetalleProductoEnFactura a, Producto b
WHERE a.idProducto = b.idProducto
AND a.idProducto = $P{idProducto}
GROUP BY a.idProducto, b.NombreProducto
ORDER BY SUM(a.NumUnidades) DESC;]]>
	</queryString>
	<field name="idProducto" class="java.lang.Integer"/>
	<field name="NombreProducto" class="java.lang.String"/>
